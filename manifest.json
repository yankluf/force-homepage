{
	"manifest_version": 3,
	"name": "Force Homepage",
	"description": "Forces a New Tab page as the homepage. Useful in browsers managed by organizations.",
    "version": "1.0.0",
	"icons": {
		"24": "icons/icon-24.png",
		"64": "icons/icon-64.png",
		"128": "icons/icon-128.png"
	},
	"permissions": ["tabs", "activeTab", "storage"],
	"action": {
        "default_title": "Force Homepage"
    },
	"background": {
		"service_worker": "background.js"
	}
}